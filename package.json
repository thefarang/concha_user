{
  "name": "concha_user",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "init-users": "node ./src/scripts/init-users.js",
    "init-roles": "node ./src/scripts/init-roles.js",
    "debug": "./node_modules/.bin/nodemon ./src/bin/startup.js",
    "test": "./node_modules/.bin/mocha \"src/test/routes/*.js\"",
    "test-cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --timeout 10000 \"src/test/routes/*.js\"",
    "test-coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "debug": "2.6.3",
    "express": "4.15.2",
    "mongoose": "4.11.12",
    "config": "1.26.2",
    "bunyan": "1.8.12"
  },
  "devDependencies": {
    "standard": "10.0.3",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "mocha": "3.5.3",
    "nodemon": "1.12.1",
    "istanbul": "0.4.5",
    "coveralls": "3.0.0",
    "mocha-lcov-reporter": "1.3.0"
  },
  "standard": {
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ]
  }
}
